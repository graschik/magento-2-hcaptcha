<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="h_captcha" type="host">hcaptcha.com</value>
                <value id="h_captcha_allsubdomains" type="host">*.hcaptcha.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="h_captcha" type="host">hcaptcha.com</value>
                <value id="h_captcha_allsubdomains" type="host">*.hcaptcha.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="h_captcha" type="host">hcaptcha.com</value>
                <value id="h_captcha_allsubdomains" type="host">*.hcaptcha.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="h_captcha" type="host">hcaptcha.com</value>
                <value id="h_captcha_allsubdomains" type="host">*.hcaptcha.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
