<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <recaptcha_frontend>
            <h_captcha_failure_messages>
                <validation_failure_message>hCAPTCHA verification failed.</validation_failure_message>
                <technical_failure_message>Something went wrong with hCAPTCHA. Please contact the store owner.</technical_failure_message>
            </h_captcha_failure_messages>
        </recaptcha_frontend>
        <recaptcha_backend>
            <h_captcha_failure_messages>
                <validation_failure_message>hCAPTCHA verification failed.</validation_failure_message>
                <technical_failure_message>Something went wrong with hCAPTCHA. Please contact the store owner.</technical_failure_message>
            </h_captcha_failure_messages>
        </recaptcha_backend>
    </default>
</config>
